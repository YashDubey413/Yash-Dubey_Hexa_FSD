package employee;

import java.util.Date;
import java.util.HashSet;
import java.util.Objects;

public class Patient {
	private int patientId;
	private String patientName;
	private String dob;
	private long patientContact;
	private HashSet<Integer> claimId;
	
	
	public Patient(int patientId, String patientName, String dob, long patientContact, HashSet claimId) {
		super();
		this.patientId = patientId;
		this.patientName = patientName;
		this.dob = dob;
		this.patientContact = patientContact;
		this.claimId = claimId;
	}


	public Patient() {
		
	}
	
	
	public int getPatientId() {
		return patientId;
	}
	public void setPatientId(int patientId) {
		this.patientId = patientId;
	}
	public String getPatientName() {
		return patientName;
	}
	public void setPatientName(String patientName) {
		this.patientName = patientName;
	}
	public String getDob() {
		return dob;
	}
	public void setDob(String dob) {
		this.dob = dob;
	}
	public long getPatientContact() {
		return patientContact;
	}
	public void setPatientContact(long patientContact) {
		this.patientContact = patientContact;
	}
	

	public HashSet<Integer> getClaimId() {
		return claimId;
	}


	public void setClaimId(HashSet<Integer> claimId) {
		this.claimId = claimId;
	}


	@Override
	public String toString() {
		return "Patient [patientId=" + patientId + ", patientName=" + patientName + ", dob=" + dob + ", patientContact="
				+ patientContact + "]\n";
	}


	@Override
	public int hashCode() {
		return Objects.hash(patientId);
	}


	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Patient other = (Patient) obj;
		return patientId == other.patientId;
	}
	
	

}
