package map;

import java.util.HashMap;
import java.util.Map;

public class MapDemo {
	
	public static void main(String[] args) {
		Map <Integer,String>map=new HashMap<Integer,String>();
		
		map.put(101, "Yash");
		map.put(103, "ram");
		map.put(102, "ravi");
		map.put(100, "Kumar");
		
		System.out.println(map);
	}

}
